{
  "$schema": "./node_modules/@openapitools/openapi-generator-cli/config.schema.json",
  "spaces": 2,
  "generator-cli": {
    "version": "7.7.0",
    "generators": {
      "all": {
        "inputSpec": "http://localhost:8091/v3/api-docs",
        "output": "tmp/open-api/all",
        "generatorName": "typescript-angular",
        "additionalProperties": {
          "fileNaming": "kebab-case",
          "modelPropertyNaming": "camelCase",
          "nullSafeAdditionalProps": "true",
          "supportsES6": "true",
          "apiModulePrefix": "Porcin"
        }
      },
      "porcin-v1": {
        "inputSpec": "http://localhost:8091/v3/api-docs/module_porcin_v1",
        "output": "projects/analytics/modules/porcin/infrastructure/clients/src/lib/open-api/porcin-v1",
        "generatorName": "typescript-angular",
        "additionalProperties": {
          "fileNaming": "kebab-case",
          "modelPropertyNaming": "camelCase",
          "nullSafeAdditionalProps": "true",
          "supportsES6": "true",
          "apiModulePrefix": "Porcin"
        }
      }
    }
  }
}
